global string $Dave[];
global string $Alice[];

global string $Dave[]; 
$Dave = `polyCube -w 1 -h 1 -d 1 -name "Dave"`;

global string $Dave[]; 
select $Dave;
move 2 2 2;


global string $Alice[];
$Alice = `polySphere -r 1 -sx 20 -sy 20 -ax 0 1 0 -cuv 2 -ch 1 -name "Alice"`;

global string $Alice[];
select $Alice;
move -r 0 1;

global string $Alice[];
global string $Dave[];
select $Dave $Alice;
group -name "Friends";


select Friends;
rotate -r 90;
